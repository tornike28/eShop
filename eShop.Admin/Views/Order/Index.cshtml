@model List<eShop.DataTransferObject.OrderQueryDTO>

@{ if (Model is not null)
    {
        <table id="example" class="table table-striped table-bordered nowrap" style="width:100%">
            <thead>
                <tr>
                    <th>OrderID</th>
                    <th>Mail</th>
                    <th>Address</th>
                    <th>OrderStatus</th>
                    <th>TotalPrice</th>
                    <th>DateCreated</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
<tr>
    <td>@item.OrderID</td>
    <td>@item.UserMail</td>
    <td>@item.UserAddress</td>
    <td>@item.OrderStatus</td>
    <td>@item.TotalPrice</td>
    <td>@item.DateCreated</td>
    <td>
    </td>
</tr>}
            </tbody>
        </table>
    }
    else
    {
        <h1>There is not Order Yet</h1>
    }}
